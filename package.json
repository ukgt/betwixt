{
  "name": "betwixt",
  "version": "1.0.0",
  "description": "Mapping App that provides mid-point distance and points of interest",
  "engines": {
    "node": "10.15.0",
    "npm": "6.4.1"
  },
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ukgt/betwixt.git"
  },
  "author": "Mila",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ukgt/betwixt/issues"
  },
  "homepage": "https://github.com/ukgt/betwixt#readme",
  "dependencies": {
    "antd": "^3.16.3",
    "body-parser": "^1.20.1",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.18.2",
    "express-jwt": "^5.3.1",
    "helmet": "^3.16.0",
    "if-env": "^1.0.4",
    "jwks-rsa": "^1.4.0",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "pusher": "^2.2.0",
    "pusher-js": "^4.4.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "nodemon": "^1.18.11"
  }
}
